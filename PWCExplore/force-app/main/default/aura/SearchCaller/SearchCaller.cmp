<aura:component implements="lightning:isUrlAddressable" controller="SearchCallerController" description="this component is used for contact page redirection"> 
    
    <aura:handler name="init" value="{!this}" action="{!c.onPageReferenceChange}"/>
    <!-- Add a "change" handler to handle all changes to the "v.pageReference" value -->
    <aura:handler name="change" value="{!v.pageReference}" action="{!c.onPageReferenceChange}"/>
    
</aura:component>